{
  "name": "plugin-doc-site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npm run fetch-docs && next build",
    "build:cache": "npm run fetch-docs:cache && next build",
    "build:static": "npm run fetch-docs && next build",
    "dev": "next dev",
    "start": "serve out",
    "fetch-docs": "tsx scripts/fetch-docs.ts",
    "fetch-docs:cache": "tsx scripts/fetch-docs.ts --cache-only",
    "types:check": "fumadocs-mdx && next typegen && tsc --noEmit",
    "postinstall": "fumadocs-mdx"
  },
  "dependencies": {
    "@orama/orama": "^3.1.18",
    "fumadocs-core": "16.4.7",
    "fumadocs-mdx": "14.2.5",
    "fumadocs-ui": "16.4.7",
    "lucide-react": "^0.562.0",
    "next": "16.1.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@octokit/rest": "^22.0.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/mdx": "^2.0.13",
    "@types/node": "^25.0.5",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "postcss": "^8.5.6",
    "serve": "^14.2.5",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
